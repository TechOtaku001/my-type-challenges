{
  "name": "my-type-challenges",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@6.32.3",
  "scripts": {
    "readme": "esno ./scripts/readme.ts",
    "build": "esno ./scripts/build.ts",
    "lint": "eslint .",
    "translate": "esno ./scripts/translate-cli.ts",
    "utils:release": "pnpm -C utils release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechOtaku001/my-type-challenges.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TechOtaku001/my-type-challenges/issues"
  },
  "homepage": "https://github.com/TechOtaku001/my-type-challenges#readme",
  "dependencies": {
    "@type-challenges/utils": "^0.1.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "lz-string": "^1.4.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0",
    "typescript": "^4.7.4",
    "utility-types": "^3.10.0"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts",
    "rules": {
      "import/first": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-use-before-define": "off",
      "no-unused-expressions": "off",
      "prefer-const": "off",
      "no-alert": "off",
      "@typescript-eslint/consistent-type-definitions": "off"
    }
  }
}
